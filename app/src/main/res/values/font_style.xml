<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Regular">
        <item name="android:fontFamily">@font/sf_pro_regular</item>
        <item name="android:textFontWeight" tools:ignore="NewApi">400</item>
        <item name="android:textColor">?attr/appTextPrimary</item>
    </style>

    <style name="Medium">
        <item name="android:fontFamily">@font/sf_pro_medium</item>
        <item name="android:textFontWeight" tools:ignore="NewApi">500</item>
        <item name="android:textColor">?attr/appTextPrimary</item>
    </style>

    <style name="SemiBold">
        <item name="android:fontFamily">@font/sf_pro_semibold</item>
        <item name="android:textFontWeight" tools:ignore="NewApi">600</item>
        <item name="android:textColor">?attr/appTextPrimary</item>
    </style>

    <style name="Bold">
        <item name="android:fontFamily">@font/sf_pro_bold</item>
        <item name="android:textFontWeight" tools:ignore="NewApi">700</item>
        <item name="android:textColor">?attr/appTextPrimary</item>
    </style>
</resources>